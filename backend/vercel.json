{
  "version": 2,
  "headers": [
    {
      "key": "Access-Control-Allow-Origin",
      "value": "https://socialchatapp.vercel.app"
    },
    { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS, PUT, DELETE" },
    {
      "key": "Access-Control-Allow-Headers",
      "value": "Content-Type, Authorization"
    }
  ],
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
